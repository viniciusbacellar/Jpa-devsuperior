package com.devsuperior.aulajparepository.entities;

public class User {
	
	private Long id;
	private String name;
	private String email;
	private Double salary;
	
	public User() {
	}

	public User(Long id, String name, String email, Double salary) {
		this.id = id;
		this.name = name;
		this.email = email;
		this.salary = salary;
	}
	
	

}
